#include <stdio.h>

int main() {
    int n, i;
    long long product = 1;  // Use long long to handle large products

    printf("Enter a number n: ");
    scanf("%d", &n);

    for(i = 2; i <= n; i += 2) {  // Start from 2 and increment by 2
        product *= i;
    }

    printf("Product of even numbers from 1 to %d is: %lld\n", n, product);

    return 0;
}
