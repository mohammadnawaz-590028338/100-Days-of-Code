#include <stdio.h>

int main() {
    int i, j;
    
    // First part - 1 star
    for(i = 1; i <= 1; i++) {
        printf("*\n");
    }
    printf("\n");

    // Second part - 2 stars
    for(i = 1; i <= 2; i++) {
        printf("*\n");
    }
    printf("\n");

    // Third part - 5 stars
    for(i = 1; i <= 5; i++) {
        printf("*\n");
    }
    printf("\n");

    // Fourth part - 3 stars
    for(i = 1; i <= 3; i++) {
        printf("*\n");
    }
    printf("\n");

    // Fifth part - 1 star
    for(i = 1; i <= 1; i++) {
        printf("*\n");
    }

    return 0;
}
