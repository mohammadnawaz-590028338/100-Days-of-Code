#include <stdio.h>

int main() {
    int num1, num2, lcm;

    // Input two numbers
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);

    // Find the greater number to start checking for LCM
    lcm = (num1 > num2) ? num1 : num2;

    // Keep increasing lcm until it is divisible by both numbers
    while (1) {
        if (lcm % num1 == 0 && lcm % num2 == 0) {
            break;  // Found the LCM
        }
        lcm++;  // Increment lcm
    }
