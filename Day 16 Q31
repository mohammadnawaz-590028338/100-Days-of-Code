#include <stdio.h>

int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);

    if (n == 0) {
        printf("Binary: 0\n");
        return 0;
    }

    int binary = 0, place = 1, temp = n;

    while (temp > 0) {
        int remainder = temp % 2;
        binary += remainder * place;
        place *= 10;
        temp /= 2;
    }

    printf("Binary: %d\n", binary);
    return 0;
}
